{
  "name": "playback",
  "version": "1.0.0",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:all": "npm run build && npm run postbuild",
    "build:worker": "tsc --project tsconfig.worker.json",
    "build:ocr": "cd src/services && swift build -c release",
    "postbuild": "npm run build:ocr && npm run build:worker && npm run copy:swift",
    "copy:swift": "mkdir -p dist/bin && cp src/services/.build/release/OCRServer dist/bin/ && cp src/services/.build/release/Eventlogger dist/bin/",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-builder install-app-deps",
    "package:mac": "npm run build:all && cross-env NODE_ENV=production electron-builder --mac --x64 --config build.config.js",
    "package:dir": "npm run build:all && electron-builder --dir"
  },
  "keywords": [],
  "author": "Fabian Weber",
  "license": "ISC",
  "description": "Playback Alpha is a data capture tool for capturing and analyzing user behavior on their desktop.",
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "@types/aws-sdk": "^0.0.42",
    "@types/better-sqlite3": "^7.6.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^2.0.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^2.3.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/vite": "^4.1.3",
    "@types/screenshot-desktop": "^1.12.3",
    "@types/sharp": "^0.31.1",
    "amazon-cognito-identity-js": "^6.3.15",
    "async-limiter": "^2.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "node-fetch": "^3.3.2",
    "piscina": "^4.9.2",
    "screenshot-desktop": "^1.15.1",
    "sharp": "^0.33.5",
    "undici": "^6.21.2"
  }
}
